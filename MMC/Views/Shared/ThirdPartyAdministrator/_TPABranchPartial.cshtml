@using MMCApp.Infrastructure.Global;
@using System.Web.Script.Serialization;
@using (Html.BeginForm(GlobalConst.Actions.ThirdPartyAdministratorController.SaveTPABranchDetail, GlobalConst.Controllers.ThirdPartyAdministrator, System.Web.Mvc.FormMethod.Post, new
{
    id = "frmTPABranchPartial",
    enctype = "multipart/form-data",
    data_bind = "ajaxForm : { success : function(responseText ,statusText,xhr,$form){AddTPABranchSuccess(responseText);},beforeSubmit : TPABranchInfoFormBeforeSubmit}"
}))
{
    <div class="row">
        <div class="col-lg-12 client-accordian">
            <div class="panel panel-default">
                <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                    <div class="panel-body row">
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Name :</label>
                                <div class="col-sm-9">
                                    <input type="hidden" class="form-control" name="TPAID" id="TPAID" data-bind="value:TPAID">
                                    <input type="hidden" class="form-control" name="TPABranchID" id="TPABranchID" data-bind="value:TPABranchID">
                                    <input type="text" class="form-control" name="TPABranchName" id="TPABranchName" required="required" maxlength="25" data-bind="value:TPABranchName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Address :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="TPABranchAddress" id="TPABranchAddress" required="required" maxlength="50" data-bind="value:TPABranchAddress,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Address 2 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="TPABranchAddress2" id="TPABranchAddress2" maxlength="50" data-bind="value:TPABranchAddress2,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>City :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" maxlength="50" name="TPABranchCity" id="TPABranchCity" required="required" data-bind="value:TPABranchCity,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>State :</label>
                                <div class="col-sm-9">
                                    <select class="form-control" name="TPABranchStateId" data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItem1,jqBootstrapValidation:{}"></select>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control zipMaskformat" name="TPABranchZip" id="TPABranchZip" required="required" data-bind="value:TPABranchZip,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" name="TPABranchPhone" id="TPABranchPhone" required="required" data-bind="value:TPABranchPhone,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Fax :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" name="TPABranchFax" id="TPABranchFax" required="required" data-bind="value:TPABranchFax,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- col-lg-12 -->
    </div><!-- row -->
    <div class="row">
        <div class="col-sm-12">
            @*<button class="btn btn-default padding-btn color-black pull-right margin-left-20 margin-btm-30" type="button">Cancel</button>*@
            <button class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>
        </div>
    </div>
}
