@using MMCApp.Infrastructure.Global;
@using System.Web.Script.Serialization;
@using (Html.BeginForm(GlobalConst.Actions.ThirdPartyAdministratorController.SaveTPADetail, GlobalConst.Controllers.ThirdPartyAdministrator, System.Web.Mvc.FormMethod.Post, new
{
    id = "frmTPAPartial",
    enctype = "multipart/form-data",
    data_bind = "ajaxForm : { success : function(responseText ,statusText,xhr,$form){AddTPASuccess(responseText);},beforeSubmit : TPAInfoFormBeforeSubmit}"
}))
{
    <div class="row">
        <div class="col-lg-12 client-accordian">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="heading11">
                    <h4 class="panel-title">
                        <a>
                            Third Party Administrator Demography
                        </a>
                    </h4>
                </div>
                <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                    <div class="panel-body row">
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Name :</label>
                                <div class="col-sm-9">
                                    <input type="hidden" class="form-control" name="TPAID" id="TPAID" data-bind="value:TPAID">
                                    <input type="text" class="form-control" name="TPAName" id="TPAName" required="required" maxlength="25" data-bind="value:TPAName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Address :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="TPAAddress" id="TPAAddress" required="required" maxlength="50" data-bind="value:TPAAddress,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Address 2 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="TPAAddress2" id="TPAAddress2" maxlength="50" data-bind="value:TPAAddress2,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>City :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" maxlength="50" name="TPACity" id="TPACity" required="required" data-bind="value:TPACity,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>State :</label>
                                <div class="col-sm-9">
                                    <select class="form-control" name="TPAStateId" data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItem,jqBootstrapValidation:{}"></select>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control zipMaskformat" name="TPAZip" id="TPAZip" required="required" data-bind="value:TPAZip,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Phone :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" name="TPAPhone" id="TPAPhone" required="required" data-bind="value:TPAPhone,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Fax :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" name="TPAFax" id="TPAFax" required="required" data-bind="value:TPAFax,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- col-lg-12 -->
    </div><!-- row -->
    <div class="row">
        <div class="col-sm-12">
            @*<button class="btn btn-default padding-btn color-black pull-right margin-left-20 margin-btm-30" type="button">Cancel</button>*@
            <button class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>
        </div>
    </div>
}
