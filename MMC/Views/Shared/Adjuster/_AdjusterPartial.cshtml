@model MMCApp.Domain.Models.AdjusterModel.Adjuster  
 @using MMCApp.Infrastructure.Global;
    @using System.Web.Script.Serialization
    @using (Html.BeginForm(GlobalConst.Actions.AdjusterController.SaveAdjusterDetail, GlobalConst.Controllers.Adjuster, FormMethod.Post, new
    {
        id = "frmSaveAdjusterDetails",
        enctype = "multipart/form-data",
        data_bind = "ajaxForm:{ success: function(responseText, statusText, xhr, $form){ AddAdjusterSuccess(responseText);},beforeSubmit: AdjusterFormBeforeSubmit}"
    }))
    {
         
        
    <div class="row">
        <div class="col-lg-12 client-accordian">
            <div class="panel panel-default">                                                                                                           
                <div class="panel-heading" role="tab" id="heading11">
                    <h4 class="panel-title">
                        <a>
                            Adjuster Demography
                        </a>
                    </h4>
                </div>
                <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                    <div class="panel-body row">
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>First Name :</label>
                                <div class="col-sm-9">
                                    <input type="hidden" name="AdjusterID" data-bind="value:AdjusterID" />
                                    <input type="text" class="form-control" id="AdjFirstName" required name="AdjFirstName" maxlength="50" data-bind="value:AdjFirstName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Last Name :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="AdjLastName" required name="AdjLastName" maxlength="50" data-bind="value:AdjLastName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Address1 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="AdjAddress1" required name="AdjAddress1" maxlength="50" data-bind="value:AdjAddress1,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                      <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Address2 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="AdjAddress2" maxlength="50" name="AdjAddress2" data-bind="value:AdjAddress2,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>City :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="AdjCity" maxlength="50" required name="AdjCity" data-bind="value:AdjCity,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>State :</label>
                                <div class="col-sm-9">  
                                    <select class="form-control" name="AdjStateId" required data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItem,jqBootstrapValidation:{}"></select>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control zipMaskformat"  id="AdjZip" required name="AdjZip" data-bind="value:AdjZip,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Phone :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" id="AdjPhone"  required name="AdjPhone" data-bind="value:AdjPhone,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>


                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Fax :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" id="AdjFax" required name="AdjFax" data-bind="value:AdjFax,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>E-mail:</label>
                                <div class="col-sm-9">
                                    <input type="email" class="form-control" id="AdjEMail" maxlength="50" required name="AdjEMail" data-bind="value:AdjEMail,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Claim Administrator:</label>
                                <div class="col-sm-9">
                                    <select class="form-control" name="ClientID" required data-bind="options: Clients, optionsCaption:'Select',  optionsText: 'ClaimAdministratorName', optionsValue: 'ClientID', value: selectedClientItem,jqBootstrapValidation:{}"></select>
                                </div>
                            </div>
                        </div>  
                    </div>
                </div>
            </div>

        </div><!-- col-lg-12 -->
    </div><!-- row -->
    <div class="row">
        <div class="col-sm-12">

            @*<button class="btn btn-default padding-btn color-black pull-right margin-left-20 margin-btm-30" type="button">Cancel</button>*@
            <button class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>

        </div>
    </div>
    }
