@model MMCApp.Domain.Models.InsurerModel.Insurer  
 @using MMCApp.Infrastructure.Global;
    @using System.Web.Script.Serialization
<div id="InsurerSaveDetailDiv">
    @using (Html.BeginForm(GlobalConst.Actions.InsurerController.SaveInsurerDetail, GlobalConst.Controllers.Insurer, FormMethod.Post, new
    {
        id = "frmSaveInsurerDetails",
        enctype = "multipart/form-data",
        data_bind = "ajaxForm:{ success: function(responseText, statusText, xhr, $form){ AddInsurerSuccess(responseText);},beforeSubmit: InsurerFormBeforeSubmit}"
    }))
    {

        <div class="row">
            <div class="col-lg-12 client-accordian">
                <div class="panel panel-default">
                    <div class="panel-heading" role="tab" id="heading11">
                        <h4 class="panel-title">
                            <a>
                                Insurer Demography
                            </a>
                        </h4>
                    </div>
                    <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                        <div class="panel-body row">
                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Insurer Name : </label>

                                    <div class="col-sm-9">
                                        <input type="hidden" name="InsurerID" id="InsurerID" data-bind="value:InsurerID" />
                                        <input type="text" class="form-control" maxlength="50" required id="InsName" name="InsName" data-bind="value:InsName,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="InsContactName" class="col-sm-3 control-label"><span class="required-link"></span>Contact Name :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" id="InsContactName" maxlength="20" name="InsContactName" data-bind="value:InsContactName,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="InsAdminextention" class="col-sm-3 control-label"><span class="required-link"></span>Phone Extension :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control number" id="InsPhoneExtension" maxlength="20" name="InsPhoneExtension" data-bind="value:InsPhoneExtension,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Address 1 :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" maxlength="50" id="InsAddress1" required name="InsAddress1" data-bind="value:InsAddress1,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label">Address 2 :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" maxlength="50" id="InsAddress2" name="InsAddress2" data-bind="value:InsAddress2,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>City :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" maxlength="50" id="InsCity" required name="InsCity" data-bind="value:InsCity,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>State :</label>
                                    <div class="col-sm-9">
                                        <select class="form-control" name="InsStateId" required data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItem,jqBootstrapValidation:{}"></select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control zipMaskformat" maxlength="10" id="InsZip" required name="InsZip" data-bind="value:InsZip,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link"></span>Phone :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control phoneMaskformat" maxlength="14" id="InsPhone" name="InsPhone" data-bind="value:InsPhone,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>


                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link"></span>Fax :</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control phoneMaskformat" maxlength="14" id="InsFax" name="InsFax" data-bind="value:InsFax,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <div class="form-group row label-font clearfix">
                                    <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link"></span>E-mail :</label>
                                    <div class="col-sm-9">
                                        <input type="email" class="form-control" id="InsEMail" maxlength="50" name="InsEMail" data-bind="value:InsEMail,jqBootstrapValidation: {}">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div><!-- col-lg-12 -->
        </div><!-- row -->
        <div class="row">
            <div class="col-sm-12">

                @*<button class="btn btn-default padding-btn color-black pull-right margin-left-20 margin-btm-30" type="button">Cancel</button>*@
                <button class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>

            </div>
        </div>

    }


    @{Html.RenderPartial("~/Views/Shared/Insurer/_InsuranceBranchPartial.cshtml");}
</div>                 