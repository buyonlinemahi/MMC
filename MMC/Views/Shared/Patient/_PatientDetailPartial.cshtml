@using System.Web.Script.Serialization
@using MMCApp.Infrastructure.Global;
@using (Html.BeginForm(GlobalConst.Actions.PatientController.SavePatientDetail, GlobalConst.Controllers.Patient, FormMethod.Post, new
{
id = "frmPatientSaveDetail",
enctype = "multipart/form-data",
data_bind = "ajaxForm : { success : function(responseText ,statusText,xhr,$form){AddPatientSuccess(responseText);},beforeSubmit : AddPatientFormBeforeSubmit}"

}))
{
   
<div class="row">
    <div class="col-lg-12 client-accordian">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="heading11">
                <h4 class="panel-title">
                    <a>
                        Patient Demography
                    </a>
                </h4>
            </div>
            <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                <div class="panel-body row">
                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient First Name :</label>
                            <div class="col-sm-9">
                                <input type="hidden" name="objPatientModel.PatientID" id="PatientID" data-bind="value:PatientID" />
                                <input type="text" class="form-control" id="PatFirstName" name="objPatientModel.PatFirstName" required="required" maxlength="25" data-bind="value:PatFirstName,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient Last Name :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="PatLastName" name="objPatientModel.PatLastName" required="required" maxlength="25" data-bind="value:PatLastName,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label">Patient SSN :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control ssn" id="PatSSN" name="objPatientModel.PatSSN" maxlength="11" data-bind="value:PatSSN,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient DOB :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control datepicker" id="PatDOB" name="objPatientModel.PatDOB" required="required" data-bind="value:PatDOB,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>



                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient Age :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control number" id="PatAge"  disabled data-bind="jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient Gender :</label>
                            <div class="col-sm-9">
                                <select class="form-control" name="objPatientModel.PatGender" data-bind="value:PatGender,jqBootstrapValidation: {}">
                                    <option value="Male">Male</option>
                                    <option value="Female">Female</option>
                                    <option value="Other">Other</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient Address :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="PatAddress1" name="objPatientModel.PatAddress1" required="required" maxlength="50" data-bind="value:PatAddress1,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label">Patient Address 2 :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="PatAddress2" name="objPatientModel.PatAddress2"  maxlength="50" data-bind="value:PatAddress2,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient City :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="PatCity" name="objPatientModel.PatCity" required="required" maxlength="50" data-bind="value:PatCity,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient State :</label>
                            <div class="col-sm-9">
                                <select class="form-control" name="objPatientModel.PatStateId" required data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItemState,jqBootstrapValidation: {}"></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Patient Zip :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control zipMaskformat" id="PatZip" name="objPatientModel.PatZip" required="required" maxlength="10" data-bind="value:PatZip,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"> Patient Phone :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control phoneMaskformat" id="PatPhone" name="objPatientModel.PatPhone"  maxlength="14" data-bind="value:PatPhone,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"> Patient Fax :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control phoneMaskformat" id="PatFax" name="objPatientModel.PatFax" maxlength="14" data-bind="value:PatFax,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"> Patient E-mail :</label>
                            <div class="col-sm-9">
                                <input type="email" class="form-control" id="PatEMail" name="objPatientModel.PatEMail" maxlength="50" data-bind="value:PatEMail,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"> Ethinic Background :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="PatEthnicBackground" name="objPatientModel.PatEthnicBackground"  maxlength="50" data-bind="value:PatEthnicBackground,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label">Primary Language :</label>
                            <div class="col-sm-9">
                                <select class="form-control" name="objPatientModel.PatPrimaryLanguageId" data-bind="options: PatPrimaryLanguages, optionsText: 'LanguageName', optionsValue: 'LanguageID', value: selectedItem, jqBootstrapValidation: {}"></select>
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label">Secondary Language :</label>
                            <div class="col-sm-9">
                                <select class="form-control" name="objPatientModel.PatSecondaryLanguageId" required data-bind="options: PatSecondaryLanguages, optionsText: 'LanguageName', optionsValue: 'LanguageID', value: selectedItemSeconday,jqBootstrapValidation: {}"></select>
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label">Marital Status :</label>
                            <div class="col-sm-9">
                                <select class="form-control" name="objPatientModel.PatMaritalStatus" data-bind="value:PatMaritalStatus,jqBootstrapValidation: {}">
                                    <option value="Married">Married</option>
                                    <option value="Unknown" selected="selected">Unknown</option>
                                    <option value="Unmarried">Unmarried</option>
                                    <option value="Widowed">Widowed</option>
                                </select>
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <div class="form-group row label-font clearfix">
                            <label for="inputEmail3" class="col-sm-3 control-label"> Medicare Eligible :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" readonly="readonly" id="PatMedicareEligible" name="objPatientModel.PatMedicareEligible" maxlength="50" data-bind="value:PatMedicareEligible,jqBootstrapValidation: {}">
                            </div>
                        </div>
                    </div>



                    <div class="clearfix"></div>


                </div>
            </div>
        </div>
    </div><!-- col-lg-12 -->
</div>

 <div class="row">
        <div class="col-sm-12">
            <button id="SavePatientDetail" class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>
        </div>
 </div>
}
<div class="row DivHide hide">
    <div class="col-sm-12">
        @{Html.RenderPartial("~/Views/Shared/Patient/_PatientCurrentMedicalConditionsPartial.cshtml");}
        <input type="hidden" name="objPatientModel.PatCurrentMedicalConditionIds" id="PatCurrentMedicalConditionIds" data-bind="value:PatCurrentMedicalConditionIds" />
    </div>
</div>