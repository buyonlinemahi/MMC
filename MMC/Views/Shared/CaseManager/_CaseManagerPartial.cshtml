@model MMCApp.Domain.Models.CaseManagerModel.CaseManager
@using MMCApp.Infrastructure.Global;
@using System.Web.Script.Serialization
@using (Html.BeginForm(GlobalConst.Actions.CaseManagerController.SaveCaseManagerDetail, GlobalConst.Controllers.CaseManager, FormMethod.Post, new
    {
        id = "frmSaveCaseManagerDetails",
        enctype = "multipart/form-data",
        data_bind = "ajaxForm:{ success: function(responseText, statusText, xhr, $form){ AddCaseManagerSuccess(responseText);},beforeSubmit: CaseManagerFormBeforeSubmit}"
    }))
{

    <div class="row">
        <div class="col-lg-12 client-accordian">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="heading11">
                    <h4 class="panel-title">
                        <a>
                            Case Manager Demography
                        </a>
                    </h4>
                </div>
                <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                    <div class="panel-body row">
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>First Name : </label>

                                <div class="col-sm-9">
                                    <input type="hidden" name="CaseManagerID" id="CaseManagerID" data-bind="value:CaseManagerID" />
                                    <input type="text" class="form-control" maxlength="50" required id="CMFirstName" name="CMFirstName" data-bind="value:CMFirstName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="InsContactName" class="col-sm-3 control-label"><span class="required-link">*</span>Last Name :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="CMLastName" required="required" maxlength="50" name="CMLastName" data-bind="value:CMLastName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="InsAdminextention" class="col-sm-3 control-label"><span class="required-link">*</span>Address 1 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="CMAddress1" required="required" maxlength="50" name="CMAddress1" data-bind="value:CMAddress1,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Address 2 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" maxlength="50" id="CMAddress2" name="CMAddress2" data-bind="value:CMAddress2,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>City :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" maxlength="50" id="CMCity" required name="CMCity" data-bind="value:CMCity,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>State :</label>
                                <div class="col-sm-9">
                                    <select class="form-control" name="CMStateId" required data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItem,jqBootstrapValidation:{}"></select>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control zipMaskformat" maxlength="10" id="CMZip" required name="CMZip" data-bind="value:CMZip,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Phone :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" maxlength="14" id="CMPhone"  name="CMPhone" data-bind="value:CMPhone,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>


                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Fax :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" maxlength="14" id="CMFax"  name="CMFax" data-bind="value:CMFax,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">E-mail :</label>
                                <div class="col-sm-9">
                                    <input type="email" class="form-control" id="CMEmail" maxlength="50"  name="CMEmail" data-bind="value:CMEmail,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Notes :</label>
                                <div class="col-sm-9">
                                    <textarea type="text" class="form-control resize-none" id="CMNotes" name="CMNotes" rows="3"  data-bind="value:CMNotes,jqBootstrapValidation: {}"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div><!-- col-lg-12 -->
    </div><!-- row -->
    <div class="row">
        <div class="col-sm-12">

            @*<button class="btn btn-default padding-btn color-black pull-right margin-left-20 margin-btm-30" type="button">Cancel</button>*@
            <button class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>

        </div>
    </div>
}
