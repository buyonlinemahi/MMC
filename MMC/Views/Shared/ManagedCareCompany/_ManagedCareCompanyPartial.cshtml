@model MMCApp.Domain.Models.ManagedCareCompanyModel.ManagedCareCompany
@using MMCApp.Infrastructure.Global;
@using System.Web.Script.Serialization

@using (Html.BeginForm(GlobalConst.Actions.ManagedCareCompanyController.SaveCareCompanyDetail, GlobalConst.Controllers.ManagedCareCompany, FormMethod.Post, new
 {
     id = "frmCompanyPartial",
     enctype = "multipart/form-data",
     data_bind = "ajaxForm : { success : function(responseText ,statusText,xhr,$form){AddManagedCareCompanySuccess(responseText);}}"
 }))
{
       <!-- row -->
    <div class="row">
        <div class="col-lg-12 client-accordian">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="heading11">
                    <h4 class="panel-title">
                        <a>
                            Company Demography
                        </a>
                    </h4>
                </div>
                <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                    <div class="panel-body row">
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Company Name :</label>
                                <div class="col-sm-9">
                                    <input type="hidden" name="CompanyId" id="CompanyId" data-bind="value:CompanyId" />
                                    <input type="text" class="form-control" id="CompName" name="CompName" required="required" maxlength="25" data-bind="value:CompName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Address  :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="CompAddress" name="CompAddress" required="required" maxlength="50" data-bind="value:CompAddress,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Address 2 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="CompAddress2" name="CompAddress2" maxlength="50" data-bind="value:CompAddress2,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>City :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="CompCity" name="CompCity" required="required" maxlength="50" data-bind="value:CompCity,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>State :</label>
                                <div class="col-sm-9">
                                    <select class="form-control" name="CompStateId" required data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItem,jqBootstrapValidation:{}"></select>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control zipMaskformat" id="CompZip" name="CompZip" required="required" maxlength="10" data-bind="value:CompZip,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div><!-- col-lg-12 -->
    </div>
    <div class="row">
        <div class="col-sm-12">
            <button class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>

        </div>
    </div>
}
