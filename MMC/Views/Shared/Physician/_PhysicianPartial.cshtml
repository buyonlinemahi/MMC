@model MMCApp.Domain.Models.PhysicianModel.Physician
@using MMCApp.Infrastructure.Global;
@using System.Web.Script.Serialization
@using (Html.BeginForm(GlobalConst.Actions.PhysicianController.SavePhysicianDetail, GlobalConst.Controllers.Physician, FormMethod.Post, new
    {
        id = "frmPhysicianPartial",
        enctype = "multipart/form-data",
        data_bind = "ajaxForm : { success : function(responseText ,statusText,xhr,$form){AddPhysicianSuccess(responseText);},beforeSubmit : PhysicianInfoFormBeforeSubmit}"
    }))
{

    <div class="row">
        <div class="col-lg-12 client-accordian">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="heading11">
                    <h4 class="panel-title">
                        <a>Physician Demography
                        </a>
                    </h4>
                </div>
                <div id="collapse11" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading11">
                    <div class="panel-body row">
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>First Name :</label>
                                <div class="col-sm-9">
                                    <input type="hidden" name="PhysicianId" id="PhysicianId" data-bind="value:PhysicianId" />
                                    <input type="text" class="form-control" id="PhysFirstName" name="PhysFirstName" required="required" maxlength="25" data-bind="value:PhysFirstName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Last Name :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="PhysLastName" name="PhysLastName" required="required" maxlength="25" data-bind="value:PhysLastName,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Address 1 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="PhysAddress1" name="PhysAddress1" required="required" maxlength="50" data-bind="value:PhysAddress1,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Address 2 :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="PhysAddress2" name="PhysAddress2" maxlength="50" data-bind="value:PhysAddress2,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>City :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="PhysCity" name="PhysCity" required="required" maxlength="50" data-bind="value:PhysCity,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>State :</label>
                                <div class="col-sm-9">
                                    <select class="form-control" name="PhysStateId" required data-bind="options: States, optionsText: 'StateName', optionsValue: 'StateId', value: selectedItem,jqBootstrapValidation:{}"></select>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Zip :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control zipMaskformat" id="PhysZip" name="PhysZip" required="required" maxlength="10" data-bind="value:PhysZip,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Phone :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" id="PhysPhone" name="PhysPhone" maxlength="14" data-bind="value:PhysPhone,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Fax :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control phoneMaskformat" id="PhysFax" name="PhysFax" maxlength="14" data-bind="value:PhysFax,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">E-mail :</label>
                                <div class="col-sm-9">
                                    <input type="email" class="form-control" id="PhysEMail" name="PhysEMail" maxlength="50" data-bind="value:PhysEMail,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>NPI :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="PhysNPI" name="PhysNPI" required="required" maxlength="10" data-bind="value:PhysNPI,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label"><span class="required-link">*</span>Speciality :</label>
                                <div class="col-sm-9">
                                    <select class="form-control" name="PhysSpecialtyId" required data-bind="options: Specialityes, optionsText: 'SpecialtyName', optionsValue: 'SpecialtyID', value: selectedItemSpeciality,jqBootstrapValidation:{}"></select>

                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Qualification :</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="PhysQualification" name="PhysQualification" maxlength="25" data-bind="value:PhysQualification,jqBootstrapValidation: {}">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row label-font clearfix">
                                <label for="inputEmail3" class="col-sm-3 control-label">Notes :</label>
                                <div class="col-sm-9">
                                    <textarea type="text" class="form-control" id="PhysNotes" name="PhysNotes" rows="3" maxlength="100" data-bind="value:PhysNotes,jqBootstrapValidation: {}"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- col-lg-12 -->
    </div>
    <!-- row -->
    <div class="row">
        <div class="col-sm-12">
            <button class="btn btn-red padding-btn color-black pull-right margin-left-20 margin-btm-30" type="submit">Save</button>

        </div>
    </div>
}
